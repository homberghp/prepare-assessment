#!/bin/bash
for i in examsolution/*.sql;  do 
    echo "-- <expectedresult>" > rommel
    (echo "begin work;" ; cat $i; echo ";rollback;")  | psql presidentDB | grep -v "^Pager" | grep -P -v '(BEGIN|ROLLBACK)'  | sqlcomment  >> rommel
    echo "-- </expectedresult>" >>  rommel
    cat rommel >> $i
    rm rommel
done
